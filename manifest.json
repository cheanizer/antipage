{
  "manifest_version": 2,

  "name": "Unpaginate media mainstream indonesia",
  "description": "we hate pagination to increase page impresions.",
  "version": "1.0",

  "browser_action": {
   "default_icon": {
        "16": "images/white_16.png",
        "32": "images/white_32.png",
        "48": "images/white_48.png",
        "128": "images/white_128.png"
      },
   "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "parser.js", "content.js"]
    }
  ],
  "icons": {
      "16": "images/white_16.png",
		"32": "images/white_32.png",
		"48": "images/white_48.png",
		"128": "images/white_128.png"
    },
  "permissions": [
   "activeTab",
   "storage",
   "declarativeContent"
   ]
}